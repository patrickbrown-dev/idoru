<table class="table table-striped">
  <thead>
    <tr>
      <th>id</th>
      <th>title</th>
      <th>url</th>
      <th>created_at</th>
      <th>updated_at</th>
    </tr>
  </thead>

  <tbody>
    <tr>
      <td><%= @article.id %></td>
      <td><%= @article.title %></td>
      <td><%= @article.url %></td>
      <td><%= @article.created_at %></td>
      <td><%= @article.updated_at %></td>
    </tr>
  </tbody>
</table>

<article>
  <%= @article.summary.html_safe %>
</article>
