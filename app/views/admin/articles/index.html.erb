<h1>Articles</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>id</th>
      <th>title</th>
      <th>url</th>
      <th>created_at</th>
      <th>updated_at</th>
      <th>actions</th>
    </tr>
  </thead>

  <tbody>
    <% @articles.each do |article| %>
      <tr>
        <td><%= article.id %></td>
        <td><%= article.title %></td>
        <td><%= article.url %></td>
        <td><%= article.created_at %></td>
        <td><%= article.updated_at %></td>
        <td>
          <%= link_to "show", admin_article_path(article), class: "btn btn-xs btn-default" %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
