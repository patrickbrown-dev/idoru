<h1>Users</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>id</th>
      <th>email</th>
      <th>is_admin</th>
      <th>created_at</th>
      <th>updated_at</th>
      <th>actions</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.email %></td>
        <td><%= user.is_admin %></td>
        <td><%= user.created_at %></td>
        <td><%= user.updated_at %></td>
        <td>
          <%= link_to "show", admin_user_path(user), class: "btn btn-xs btn-default" %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
